/**
 * String type
 */
type String = {

  external static def apply(): String

  external def size: Int

  external def apply(index: Int): Char

  external def reverse: String

  external def +(that: String): String

  external def substring(from: Int, to: Int): String

  def substring(from: Int): String =
    this.substring(from, this.size)

}
