/**
 * Boolean type
 */
type Bool = {

  external def ==(that: Bool): Bool

  external def !=(that: Bool): Bool

  external def <(that: Bool): Bool

  external def <=(that: Bool): Bool

  external def >(that: Bool): Bool

  external def >=(that: Bool): Bool

  external def not: Bool

  external def &&(that: Bool): Bool

  external def ||(that: Bool): Bool

}
