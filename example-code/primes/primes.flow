def isPrime(n: Int, d: Int): Bool =
  if n <= 1 then
    false
  else if n == d then
    true
  else if n % d == 0 then
    false
  else
    isPrime(n, d + 1)

def isPrime(n: Int): Bool =
  isPrime(n, 2)

var n: Int = 0
var i: Int = 2
while i < 1000 do {
  if isPrime(i) then {
    printLine(i)
    n = n + 1
  }
  i = i + 1
}

printLine(n)

